mylist = []
i = 0

while i<10:
    print "Enter number"; mylist.append(input());
    i += 1

print(mylist)

while True:
    print "Number"
    num = input()

    if num == 0:
        break
    
    i = 0

    while i<10:
        if num == mylist[i]:
            print "Yes"
            break;
        if i==9:
            print "No"
        i += 1

